{
    "version":2,
    "builds":[
        {
            "src":"DonationMsysteM/wsgi.py",
            "use":"@vercel/python",
            "congig":{"maxLambdaSize":"15mb","runtime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "congig":{
                "disDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"DonationMsysteM/wsgi.py"
        }
    ]
}